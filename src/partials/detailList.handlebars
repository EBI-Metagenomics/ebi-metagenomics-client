<div class="row columns">
<h3>{{title}}</h3>
    <div class="row box">
        {{#each properties as |value key|}}
            <div class="column small-12 medium-4 large-2 text-right">{{{key}}}:</div>
            <div class="column small-12 medium-8 large-10">{{{value}}}</div>
        {{/each}}
    </div>
</div>