{
  "compilerOptions": {
    "outDir": "./dist/",
    "baseUrl": ".",
    "paths": {
      "*": [
        "*",
        "src/*",
        "public/*"
      ]
    },
    "sourceMap": true,
    "module": "es2020",
    "target": "es6",
    "lib": [
      "es2019",
      "dom",
      "esnext"
    ],
    "jsx": "react",
    "allowJs": true,
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true
  }
}